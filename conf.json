{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["src/js/"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "template": "node_modules/minami",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true
    }
}