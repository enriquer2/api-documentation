{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["src/js/decorators/rest/path.js", "src/js/decorators/rest/summary.js", "src/js/main.js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "template": "node_modules/minami",
        "encoding": "utf8",
        "destination": "docs/"
    }
}